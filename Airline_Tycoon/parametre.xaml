<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:av="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="av" x:Class="Airline_Tycoon.parametre"
        Title="Thérémine WPF - Bouge ta souris !" Height="450" Width="800">
    <Grid x:Name="MainGrid">
        <Grid.Background>
            <LinearGradientBrush StartPoint="0,0" EndPoint="1,1">
                <GradientStop Color="#FF1B1B1B" Offset="0.0"/>
                <GradientStop x:Name="DynamicColor" Color="#FF004488" Offset="1.0"/>
            </LinearGradientBrush>
        </Grid.Background>
        <ComboBox x:Name="cbVolumeson" HorizontalAlignment="Left" Margin="318,254,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="cbVolumeson_SelectionChanged"/>
    </Grid>
</Window>
